{
  "name": "@funkybob/qs",
  "version": "1.1.2",
  "description": "Simple QueryString parsing and building",
  "main": "dist/index.es.js",
  "files": [
    "index.js",
    "dist/*"
  ],
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "rollup -c",
    "prepare": "npm run build && agadoo && npm run docs",
    "docs": "esdoc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/funkybob/funky-qs.git"
  },
  "author": "Curtis Maloney",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/funkybob/funky-qs/issues"
  },
  "homepage": "https://github.com/funkybob/funky-qs#readme",
  "devDependencies": {
    "agadoo": "^1.0.0",
    "esdoc": "^1.1.0",
    "esdoc-standard-plugin": "^1.0.0",
    "rollup": "^0.65.2",
    "rollup-plugin-terser": "^2.0.2",
    "rollup-plugin-typescript2": "^0.17.0",
    "typescript": "^3.0.3"
  },
  "esdoc": {
    "source": "./src",
    "destination": "./docs",
    "plugins": [
      {
        "name": "esdoc-standard-plugin"
      }
    ]
  }
}
